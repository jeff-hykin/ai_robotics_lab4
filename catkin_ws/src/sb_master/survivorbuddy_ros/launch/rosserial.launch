<launch>
    <node pkg="rosserial_python" type="serial_node.py" name="serial_sb_0">
        <param name="port" value="/dev/ttyUSB0"/>
        <param name="baud" value="57600"/>
    </node>    
    <node pkg="rosserial_python" type="serial_node.py" name="serial_sb_1">
        <param name="port" value="/dev/ttyUSB1"/>
        <param name="baud" value="57600"/>
    </node>
    <node pkg="rosserial_python" type="serial_node.py" name="serial_sb_2">
        <param name="port" value="/dev/ttyUSB2"/>
        <param name="baud" value="57600"/>
    </node>
    <node pkg="rosserial_python" type="serial_node.py" name="serial_sb_3">
        <param name="port" value="/dev/ttyUSB3"/>
        <param name="baud" value="57600"/>
    </node>
</launch>